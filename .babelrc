{
  "presets": [
    "react",
    "es2015",
    "stage-0"
  ],
  "plugins": [
    "react-hot-loader/babel",
    "transform-regenerator",
    "transform-runtime",
    "transform-es2015-modules-commonjs",
    "transform-decorators-legacy"
  ],
  "env": {
    "production": {
      "presets": [
        ["es2015", { "modules": false }],
        "react",
        "react-optimize",
        "stage-0"
      ]
    }
  }
}